{
    "$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
    "scopeName": "source.ini",
    "fileTypes": ["conf","ini"],
    "patterns": [
        {
            "name": "block.start.open",
            "match": "^\\%5B{1}\\"
        },
        {
            "name": "block.start.close",
            "match": "\\%5D{1}$"
        },
        {
            "name": "heading",
            "match": "\\([a-zA-Z0-9_]+\\.)*[a-zA-Z0-9_]+\\"
        },
        {
            "name": "key",
            "match": "[a-zA-Z0-9_]+\\"
        },
        {
            "name": "value",
            "match": "\\=\\s*.*$"
        },
        {
            "name": "key.equals.value",
            "match": "[a-zA-Z0-9_]+\\=\\s*.*$"
        }
    ]
   
}